<% include ../partials/head.ejs %>

  <h1>Lista negra de cpf</h1>
  <p>
    <a href="/">Voltar para a home</a>
  </p>
  <table class="table table-dark">
    <tr>
      <th scope="col">Numero</th>
      <th scope="col">status</th>
      <th scope="col">ações</th>
    </tr>
    <% cpfs.map( cpf => { %>
      <tr>
        <td><%= cpf.numero %></td>
        <td><%= cpf.tipo %></td>
        <td>
          <div class="btn-group" role="group" aria-label="Basic example">
            <a type="button" class="btn btn-secondary" href="/consultas/update/<%= cpf.id %>">Editar</a>&nbsp;&nbsp;&nbsp;
            <a type="button" class="btn btn-secondary" href="/consultas/delete/<%= cpf.id %>" onclick="return confirm('Deseja realmente excluir?')">Excluir</a>
          </div>
        </td>
      </tr>
    <% }) %>
  </table>


  <% include ../partials/footer.ejs %>